{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "macroarc",
	"version": "0.1.0",
	"identifier": "com.macroarc.jumble",
	"build": {
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:2360",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "MacroArc",
				"width": 800,
				"height": 700,
				"minWidth": 700,
				"minHeight": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
